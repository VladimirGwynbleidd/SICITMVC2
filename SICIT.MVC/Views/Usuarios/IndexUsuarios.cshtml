<!-- Modal ModalAddUpdateUser-->
<div class="modal fade" style="padding-top:3%!important" id="ModalAddUpdateUsuarios" tabindex="-1" role="dialog" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="m-0 font-weight-bold text-primary" id="ModalCenterTitle"><span class="fas fa-user-o fa-lg" aria-hidden="true"></span> Registro de Usuario</h3>
                <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <!-- Overflow Hidden -->
                <form id="formNewUser" class="needs-validation form-group" autocomplete="off" registration novalidate>
                    <div class="card">
                        <div class="card-header py-3 text-center">
                            <h6 id="ModalCenterTitleH6" class="m-0 font-weight-bold text-primary"></h6>
                        </div>

                        <div class="card-body">
                            <div class="form-group">
                                <div class="input-group col-md-12">
                                    <input type="text" class="form-control" id="IdEntidadHidden" hidden>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="input-group col-md-12">
                                    <input type="text" class="form-control" id="IdTipoEntidadHidden" hidden>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="input-group col-md-12" id="IDdivIDUsuario">
                                    <input type="text" class="form-control" id="IDUsuario" maxlength="5" placeholder="ID" hidden>
                                </div>
                            </div>

                            <div class="form-row">
                                <div class="col-md-9 mb-3">
                                    <label class="my-1 mr-2" for="lblNombre">Nombre(s):</label>
                                    <input type="text" class="form-control" name="IDNombre" id="IDNombre" maxlength="20" placeholder="Nombre(s)" required>
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="col-md-9 mb-3">
                                    <label class="my-1 mr-2" for="lblApellidoPaterno">Apellido Paterno:</label>
                                    <input type="text" class="form-control" name="IDApellidoPaterno" id="IDApellidoPaterno" maxlength="20" placeholder="Apellido Paterno" required>
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="col-md-9 mb-3">
                                    <label class="my-1 mr-2" for="lblApellidoMaterno">Apellido Materno:</label>
                                    <input type="text" class="form-control" name="IDApellidoMaterno" id="IDApellidoMaterno" maxlength="20" placeholder="Apellido Materno" required>
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="col-md-9 mb-3">
                                    <label class="my-1 mr-2" for="lblDescripcion">Teléfono:</label>
                                    <input type="text" class="form-control" name="IDTelefono" id="IDTelefono" maxlength="10" placeholder="Apellido Materno" required>
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="col-md-9 mb-3">
                                    <label class="my-1 mr-2" for="lblEMail">E-Mail:</label>
                                    <input type="text" class="form-control" name="IDEMail" id="IDEMail" maxlength="20" placeholder="E-Mail" required>
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="col-md-9 mb-3">
                                    <label for="lblSelectedTipoEntidad">Tipo de Entidad</label>
                                    <select class="form-select"  id="IdSelectedTipoEntidad" name="IdSelectedTipoEntidad"></select>
                                    @*<div class="invalid-feedback">
                                            Please provide a valid city.
                                        </div>*@
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="col-md-9 mb-3">
                                    <label for="lblSelectedEntidad">Entidad</label>
                                    <select class="form-select" id="IdSelectedEntidad" name="IdSelectedEntidad"></select>
                                    @*<div class="invalid-feedback">
                                            Please provide a valid city.
                                        </div>*@
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="col-md-9 mb-3">
                                    <label for="lblSelectedArea">Area</label>
                                    <select class="form-select" id="IdSelectedArea" name="IdSelectedArea"></select>
                                    @*<div class="invalid-feedback">
                                            Please provide a valid city.
                                        </div>*@
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="col-md-9 mb-3">
                                    <label for="lblSelectedPuesto">Puesto</label>
                                    <select class="form-select" id="IdSelectedPuesto" name="IdSelectedPuesto"></select>
                                    @*<div class="invalid-feedback">
                                            Please provide a valid city.
                                        </div>*@
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="col-md-9 mb-3">
                                    <label for="lblSelectedPerfil">Perfil</label>
                                    <select class="form-select" id="IdSelectedPerfil" name="IdSelectedPerfil"></select>
                                    @*<div class="invalid-feedback">
                                            Please provide a valid city.
                                        </div>*@
                                </div>
                            </div>
                            <div class="form-row">
                                <div class="col-md-9 mb-3">
                                    <label for="lblSiglas">Usuario</label>
                                    <input type="text" class="form-control" id="IdInputUsuario" placeholder="Usuario" required>
                                    @*<div class="invalid-feedback">
                                            Please provide a valid city.
                                        </div>*@
                                </div>
                            </div>
                            <div class="form-row" id="divErrores">
                                <ul id="lista"></ul>
                            </div>

                        </div>
                    </div>
                    @*<div class="border-top my-3 border-5"></div>*@
                    <!-- Overflow Hidden -->

                </form>
            </div>
            <div class="modal-footer">
                <button class="btn btn-primary float-right" onclick="AddUpdateUsuarios();" type="submit" value="submit">Aceptar</button>
                <button class="btn btn-danger float-right" onclick="CloseModalAddUpdateUsuarios();">Cancelar</button>
            </div>
        </div>
    </div>
</div>


